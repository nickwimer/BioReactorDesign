FoamFile
{
    version   	2.0;
    format      ascii;
    class       dictionary;
    object      thermophysicalProperties.liquid;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include "$FOAM_CASE/constant/globalTransport"

thermoType
{
    type            heRhoThermo;
    mixture         multiComponentMixture;
    transport       const;
    thermo          hConst;
    equationOfState rhoConst;
    specie          specie;
    energy          sensibleInternalEnergy;
}

species
(
    H
    OH
    O2
    NH4        // ammonium
    NH3        // ammonia
    H2O
    CO2
    H2CO3      // carbonic acid
    HCO3       // bicarbonate
    H2PO4
    HPO4
    putida
    C6H6O4     // muconic acid (MAH2)
    C6H5O4     // deprotonated muconic acid (MAH-)
    C6H4O4     // muconate (MA-2)
    C6H12O6    // glucose
);

defaultSpecie H2O;

