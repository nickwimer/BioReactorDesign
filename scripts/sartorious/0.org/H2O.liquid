FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      H2O.liquid;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

#include "../constant/globalTransport"

waterLiquid #codeStream
{
    code
    #{
      os << 1.0 - $O2Liquid - $glLiquid - $putidaLiquid - $maLiquid - $hLiquid - $HPO4Liquid - $H2PO4Liquid; 
    #};
};

internalField   uniform $waterLiquid;

boundaryField
{

    #includeEtc "caseDicts/setConstraintTypes"
  
    inlet
    {
        type            fixedValue;
        value           uniform 0.5;
    }
    outlet
    {
        type            inletOutlet;
        phi             phi.liquid;
        inletValue      uniform 0.0;
        value           uniform 0.0;
    }
    walls
    {
        type            zeroGradient;
    }
    propeller
    {
        type            zeroGradient;
    }
}

